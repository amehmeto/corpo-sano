  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:inte": "jest -c jest-inte.json --runInBand --forceExit",
    "test:e2e": "jest -c ./test/jest-e2e.json --runInBand --forceExit",
    "test:cov": "jest -c jest-all.json --coverage --runInBand --forceExit",
    "fixt:gen": "ts-node test/fixtures/execute-generate-fixtures.ts",
    "fixt:del": "ts-node test/fixtures/execute-delete-fixtures.ts",
    "ci": "./tmtc-ci.sh",
    "db:seed": "node seed.js"
  },
  "dependencies": {
